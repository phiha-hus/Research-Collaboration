@Article{Ha18b,
  author   = {Phi Ha},
  title    = {On the Stability Analysis of Delay Differential-Algebraic Equations},
  journal  = {VNU Journal of Science: Mathematics - Physics},
  year     = {2018},
  volume   = {34},
  number   = {2},
  issn     = {2588-1124},
  abstract = {The stability analysis of linear time invariant delay differentialalgebraic equations (DDAEs) is analyzed. Examples are delivered to demonstrate that the eigenvalue-based approach to analyze the exponential stability of dynamical systems is not valid for an arbitrarily high index system, and hence, a new concept of weakly exponential stability (w.e.s) is proposed. Then, we characterize the w.e.s in term of a spectral condition for some special classes of DDAEs. },
